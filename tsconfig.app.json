/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.d.ts"],
  "paths": {
    "@components/*": ["src/app/components/*"],
    "@entities/*": ["src/app/entities/*"],
    "@services/*": ["src/app/services/*"],
    "@pages/*": ["src/app/pages/*"]
  }
}
